    "version": "0.2.0",
    "configurations": [
        {
            "name": "Meta debugger",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "args": [
                "configs/base_config.yaml",
                "configs/meta_config.yaml"
            ],
            "console": "integratedTerminal",
            "justMyCode": false
        },
        {
            "name": "Base debugger",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "args": [
                "configs/base_config.yaml",
            ],
            "console": "integratedTerminal",
            "justMyCode": false
        }
    ]
}
